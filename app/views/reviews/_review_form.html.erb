<% unless current_user.is_a? GuestUser %>
  <%= form_for @review, url: '#' do |form| %>
    <div class="form-group container w-75">
      <%= form.label :'Leave A Review' %>
      <%= form.text_area :body, class: 'form-control' %>
    </div>

    <div class = post-review-btn>
    <%= form.submit 'Post Review', class: 'btn btn-primary' %>
    </div>
  <% end %>
<% end %>